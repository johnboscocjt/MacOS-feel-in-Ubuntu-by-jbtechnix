{
    "files.autoSave": "afterDelay",
    "workbench.iconTheme": "material-icon-theme",
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "workbench.productIconTheme": "material-product-icons",
    "[blade]": {
        "editor.defaultFormatter": "shufo.vscode-blade-formatter"
    },
    "redhat.telemetry.enabled": true,
    "database-client.autoSync": true,
    "workbench.startupEditor": "none",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "blade": "html"
    },
    "cmake.options.statusBarVisibility": "visible",
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "html-css-class-completion.enableEmmetSupport": true,
    "tailwindCSS.emmetCompletions": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "php": true
    },
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
        "*.dart": "${capture}.g.dart",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "laravel-pint.enable": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "mssql.connectionGroups": [
        {
            "name": "ROOT",
            "id": "E2B70B65-E93D-4805-A778-2ECD38BBC431"
        }
    ],
    "mssql.connections": [],
    "[java]": {
        "editor.defaultFormatter": "Oracle.oracle-java"
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "editor.inlineSuggest.edits.showCollapsed": true,
    "editor.quickSuggestions": {
        "other": "on"
    },
    "editor.suggestOnTriggerCharacters": false,
    "editor.wordBasedSuggestions": "off",
    "C_Cpp.autocomplete": "disabled",
    "workbench.secondarySideBar.defaultVisibility": "visible",
    "editor.fontFamily": "'Cascadia Code', 'Fira Code', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontWeight": "normal",
    "editor.renderWhitespace": "selection",
}
